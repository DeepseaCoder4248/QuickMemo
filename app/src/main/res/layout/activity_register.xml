<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RegisterActivity">

    <TextView
        android:id="@+id/tv_register_activity_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="회원가입을 진행합니다"
        android:textAlignment="center"
        android:textColor="#262626"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="201dp" />

    <TextView
        android:id="@+id/tv_register_activity_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="다음"
        android:textAlignment="center"
        android:textColor="#FF5353"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="636dp" />

    <EditText
        android:id="@+id/_register_activity_birth"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:backgroundTint="#26000000"
        android:ems="10"
        android:hint="생년월일"
        android:inputType="textWebPassword"
        android:textColor="#262626"
        android:textColorHint="#26000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:inputType="textWebEmailAddress"
        tools:layout_editor_absoluteY="478dp" />

    <TextView
        android:id="@+id/tv_register_activity_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#26000000"

        android:text="내용을 모두 입력해주세요"
        android:textAlignment="center"
        android:textColor="#FF6868"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="523dp" />

    <EditText
        android:id="@+id/_register_activity_checkpw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:backgroundTint="#26000000"
        android:ems="10"
        android:hint="비밀번호 확인"
        android:inputType="textWebPassword"
        android:textColor="#262626"
        android:textColorHint="#26000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        tools:inputType="textWebEmailAddress"
        tools:layout_editor_absoluteY="433dp" />

    <EditText
        android:id="@+id/_register_activity_pw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:backgroundTint="#26000000"
        android:hint="비밀번호"
        android:inputType="textWebPassword"
        android:textColor="#262626"
        android:textColorHint="#26000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        tools:inputType="textWebEmailAddress"
        tools:layout_editor_absoluteY="388dp" />

    <EditText
        android:id="@+id/_register_activity_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginRight="60dp"
        android:backgroundTint="#26000000"
        android:ems="10"
        android:hint="이메일 또는 전화번호"
        android:inputType="textWebPassword"
        android:textColor="#262626"
        android:textColorHint="#26000000"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:inputType="textWebEmailAddress"
        tools:layout_editor_absoluteY="343dp" />

    <CheckBox
        android:id="@+id/cb_register_activity_allcheck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="약관에 모두 동의합니다"
        android:textColor="#262626"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="287dp" />

    <CheckBox
        android:id="@+id/cb_register_activity_personal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="개인정보 처리방침"
        android:textColor="#262626"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="255dp" />

    <CheckBox
        android:id="@+id/cb_register_activity_policy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이용약관"
        android:textColor="#262626"
        tools:layout_editor_absoluteX="90dp"
        tools:layout_editor_absoluteY="255dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline3"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="201dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="343dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline5"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="201dp" />


</androidx.constraintlayout.widget.ConstraintLayout>