<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PINActivity">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="448dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="233dp" />

    <TextView
        android:id="@+id/tv_reset_user_activity_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#26000000"
        android:text="PIN을 입력해주세요"
        android:textAlignment="center"
        android:textColor="#262626"
        android:textColorHint="#26000000"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="200dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="●"
        android:textColor="#CDCDCD"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="197dp"
        tools:layout_editor_absoluteY="323dp" />

    <TextView
        android:id="@+id/tv_reset_user_activity_message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="입력한 정보를 가지고 있지 않습니다"
        android:textAlignment="center"
        android:textColor="#FF6868"
        tools:layout_editor_absoluteX="102dp"
        tools:layout_editor_absoluteY="345dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PIN을 잊어버리셨나요?"
        android:textAlignment="center"
        android:textColor="#262626"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="447dp" />

    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PIN 재설정"
        android:textAlignment="center"
        android:textColor="#FF6868"
        tools:layout_editor_absoluteX="173dp"
        tools:layout_editor_absoluteY="466dp" />


</androidx.constraintlayout.widget.ConstraintLayout>